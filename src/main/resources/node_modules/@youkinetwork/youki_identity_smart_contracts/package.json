{
  "name": "@youkinetwork/youki_identity_smart_contracts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "version": "1.0.6",
  "description": "Youki Identity Truffle Project ",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:ganache": "rm -rf build && truffle migrate --reset --compile-all --network ganache",
    "migrate:rinkeby": "rm -rf build && truffle migrate --reset --compile-all --network rinkeby",
    "migrate:dev": "rm -rf build && truffle migrate --reset --compile-all --network dev",
    "lint:sol": "solium -d .",
    "lint:sol:fix": "solium -d . --fix",
    "coverage": "solidity-coverage"
  },
  "files": [
    "build",
    "contracts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youkiNetwork/youki_identity_smart_contracts.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/youkiNetwork/youki_identity_smart_contracts/issues"
  },
  "homepage": "https://github.com/youkiNetwork/youki_identity_smart_contracts#readme",
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@openzeppelin/contracts": "^4.4.0",
    "@truffle/hdwallet-provider": "^2.0.0",
    "dotenv": "^10.0.0",
    "truffle-contract-size": "^2.0.1",
    "web3": "^1.6.1"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.4.0",
    "ethlint": "^0.0.0"
  }
}
